class Math {
    init(message) {
        this.message = message;
    }
    class PI(a) {
        return 3.14 + a;
    }
    class hello(msg) {
        return "class " + msg;
    }
    class printThis() {
        return this;
    }
    hello(input) {
        return this.message + input;
    }
}
print Math.printThis();
